{
  "manifest_version": 2,
  "name": "AnyScript",
  "version": "0.0.1",
  "description": "Run script on any page",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
