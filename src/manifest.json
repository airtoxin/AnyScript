{
  "manifest_version": 2,
  "name": "AnyScript",
  "version": "0.0.1",
  "description": "Run script on any page",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  }
}
